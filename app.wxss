/**app.wxss**/
/* html转小程序 */
/**基础风格样式**/
@import '/towxml/style/main.wxss';

/**如果页面有动态主题切换，则需要将使用到的样式全部引入**/

/**主题配色（浅色样式）**/
/* @import '/towxml/style/theme/light.wxss'; */

/**主题配色（深色样式）**/
/* @import '/towxml/style/theme/dark.wxss'; */
/*清除默认样式  */
element {-webkit-overflow-scrolling: touch;}
button{
  margin: 0;
  padding: 0;
  border: 0 !important;
  outline: none;
  list-style-type:none;
  border-radius: 0;
  box-shadow: none;
  background: none;
}
/* 公共样式 */
.borderTop{
  border-top: 1px solid #e3e1ec;
}

/* 首页列表公共样式 */
image,video{
  margin: 0;
}
.bgwhite{
  background: #fff;
}
.padlf{
  padding: 0 5%;
  box-sizing: border-box;
}
.magb{
  margin-bottom: 18rpx;
}
.emptybox{
  height: 100rpx;
}
.emptybox12{
  height: 120rpx;
}
.grayemptybox{
  height: 18rpx;
  background: #e8e8e8;
  width: 100%;
}
.module{
  background: #fff;
  padding: 20rpx 5%;
  box-sizing: border-box;
  margin-bottom: 18rpx;
}
.module1{
  background: #fff;
  padding: 20rpx 5%;
  box-sizing: border-box;
}
.module2{
  background: #fff;
  padding: 40rpx 5%;
  box-sizing: border-box;
  margin-bottom: 18rpx;
  text-align: center;
}
.module_title{
  padding: 15rpx 100rpx 15rpx 0;
  position: relative;
  font-size: 36rpx;
  font-weight: bold;
  color: #000;
}
.recommend_module{
  display: flex;
  /* align-items: center; */
}
.hot_module,.recommend_module{
  display: flex;
  border-bottom: 1px solid #e5e5e5;
  padding: 25rpx 0;
}
.hot_module:last-child,.recommend_module:last-child{
  border-bottom: 0;
}
.hot_left{
  flex: 1.8;
  padding-right: 20rpx;
  box-sizing: border-box;
}
.hot_right{
  flex: 1;
  border-radius: 15rpx;
  overflow: hidden;
}
.hotimg{
  display: block;
  width: 254rpx;
  height: 192rpx;
}
.hlT1{
  font-size: 30rpx;
  color: #000;
  line-height: 46rpx;
}
.hlT2{
  font-size: 24rpx;
  color: #999;
  line-height: 35rpx;
  padding-top: 15rpx;
}
.more_icon{
  display: inline-block;
  width: 100rpx;
  height: 67rpx;
  background: url("data:image/png;base64,") no-repeat; 
  background-size: 29rpx 24rpx;
  background-position: 80% 50%;
  position: absolute;
  top: 0;
  right: 0;
}
.recommend_left{
  flex: 1;
  border-radius: 15rpx;
  overflow: hidden;
  width: 178rpx;
  height: 178rpx;
  position: relative;
}
.recommend_right{
  flex: 2.6;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.rlT1{
  font-size: 30rpx;
  position: relative;
  padding-right: 120rpx;
}
.rlT2{
  font-size: 26rpx;
  color: #6e6e6e;
  padding: 14rpx 0 14rpx 0;
  width: 470rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.peopleimg{
  display: block;
  width: 178rpx;
  height: 178rpx;
  border-radius: 15rpx;
}
.rlT2 view{
  display: inline-block;
}
.rlT2 .name{
  padding-right: 20rpx;
  margin-right: 0;
}
.xianshi{
  position: absolute;
  left: 0;
  top: 0;
  width: 58rpx;
  height: 55rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
}
.jobMarket{
  display: inline-block;
  padding: 3rpx 12rpx 6rpx;
  font-size: 22rpx;
  color: #55b387;
  border: 1px solid #55b387;
  border-radius: 10rpx;
  position: absolute;
  right: 0;
  top: 5rpx;
  max-width: 112rpx;
}
.countbox{
  text-align: right;
}
.count{
  display: inline-block;
  padding: 0 8rpx 5rpx;
  background: #ffc000;
  color: #fff;
  font-size: 22rpx;
  border-radius: 10rpx;
}
.recommend_bottom{
  position: relative;
  /* padding-right: 160rpx; */
  display: box;
}
.recommend_bottom view{
  display: inline-block;
  box-flex:1;
}
.time{
  color: #999;
  padding-left: 28rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 21rpx 21rpx;
  background-position: 0 50%;
  font-size: 22rpx;
}
.num{
  color: #999;
  padding-left: 25rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 16rpx 20rpx;
  background-position: 0 50%;
  margin-left: 30rpx;
  font-size: 22rpx;
  margin-right: 30rpx;
}
.price{
  color: #ffa537;
  font-size: 26rpx;
  position: absolute;
  right: 0;
  bottom: 0;
}
.grayprice{
  color: #999;
  text-decoration: line-through;
  margin-right: 14rpx;
}
/* 修改编辑器默认样式 */
.h2w__p{
  margin-bottom: 0!important;
}