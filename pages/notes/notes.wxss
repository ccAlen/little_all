/* pages/notes/notes.wxss */
.tipsTxt{
  font-size: 30rpx;
  color: #999;
  padding: 30rpx 0;
}
page{
  background: #fff;
}
.notes_title{
  font-size: 30rpx;
  color: #000;
  padding: 0 0 30rpx;
}
.tableview{
  border: 1px solid #e5e5e5;
  border-radius: 10rpx;
}
.trview{
  display: flex;
}
.tdview1{
  flex: 3;
  text-align: center;
  font-size: 26rpx;
  color: #666;
  border-right: 1px solid #e5e5e5;
  padding: 25rpx 10rpx;
  box-sizing: border-box;
}
.tdview2{
  flex: 1;
  text-align: center;
  font-size: 26rpx;
  color: #666;
  padding: 25rpx 10rpx;
  box-sizing: border-box;
}
.headerview{
  border-bottom: 1px solid #e5e5e5;
}
.headerview .tdview1,.headerview .tdview2{
  color: #000;
}
.detailsLink{
  display: inline-block;
  width: 28rpx;
  height: 34rpx;
  vertical-align:middle;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 28rpx 34rpx;
  background-position: 0 50%; 
}
.tips{
  font-size: 22rpx; 
  color: #999;
  line-height: 36rpx;
  padding: 40rpx 0;
}
.tipsLink{
  color: #0090ff;
  text-decoration: underline;
  display: inline-block;
}
/*20180706  */
.module_title_data{
  font-size: 36rpx;
  color: #55b387;
  padding:30rpx 0 30rpx 25rpx;
  position:relative;
  font-weight:bold;
}
.module_title_data::before{
  content: '';
  display: inline-block;
  width: 2px;
  height: 34rpx;
  border-radius: 20rpx;
  background: #55b387;
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -16rpx;
}
.have{
  display: inline-block;
  margin-left: 20rpx;
}
.tableview_data{
  border:1px solid #e5e5e5;
  border-radius:10rpx;
  padding: 0 5%;
  box-sizing: border-box;
}
.diplomaList{
  display: box;
  display: -webkit-box;
  font-size: 30rpx;
  padding: 30rpx 20rpx 30rpx 0;
  border-bottom: 1px solid #e5e5e5;
  background: url("data:image/jpg;base64,") no-repeat;
  background-size: 16rpx 31rpx;
  background-position: 100% 50%;
}
.diplomaList:last-child{
  border-bottom: 0;
}
.dl_child{
  box-flex:1;
}
.dl1{
  color: #333;
  width: 40%;
}
.dl1 text{
  color: #55b387;
}
.dl2{
  width: 12%;
  text-align: center;
}
.dl3{
  color: #ffa537;
  width: 48%;
  text-decoration: underline;
}
.elseList{
  display: box;
  display: -webkit-box;
  font-size: 30rpx;
  padding: 10rpx 0;
}
.moreList{
  font-size: 30rpx;
  padding: 30rpx 40rpx 30rpx 0;
  /* border-bottom: 1px solid #e5e5e5; */
}
.downicon{
  background: url("data:image/jpg;base64,") no-repeat;
  background-size: 38rpx 17rpx;
  background-position: 100% 50%;
}
.upicon{
  background: url("data:image/jpg;base64,") no-repeat;
  background-size: 38rpx 17rpx;
  background-position: 100% 50%;
}
/*下拉动画  */

.kind-list-item-bd {
  height: 0;
  overflow: hidden;
}
.kind-list-item-bd-show {
  height: auto;
}
.navigator-box {
  transform: translateY(-50%);
  transition: .3s;
}
.navigator-box-show {
  transform: translateY(0);
}

.elseList:last-child{
  margin-bottom: 40rpx;
}
.headericon {
  display:inline-block;
  width:32rpx;
  height:40rpx;
  vertical-align:middle;
  background:url("data:image/jpg;base64,") no-repeat;
  background-size:32rpx 40rpx;
  background-position: 0 50%;
}
