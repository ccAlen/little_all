.header {
  height: 223rpx;
  background: #fff;
  overflow: hidden;
  position: relative;
}

.header .portrait {
  float: left;
  margin-left: 30rpx;
  margin-top: 47rpx;
}

.header .portrait image {
  width: 130rpx;
  height: 130rpx;
}

.header .text-container{
  margin-left: 190rpx;
  padding-top: 70rpx;
}

.header .text-container .name{
  font-size: 40rpx;
  color: #000000;
  line-height: 40rpx;
  font-weight: bold;
}

.header .text-container .info{
  font-size: 26rpx;
  color: #999999;
  line-height: 26rpx;
  margin-top: 15rpx;
}

.header .note{
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto;
  right: 0;
  height: 54rpx;
  background: #ffecb2;
  width: 156rpx;
  border-radius: 50rpx;
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
  text-align: center;
  color: #000000;
  font-size: 26rpx;
  line-height: 54rpx;
}
.header .icon_note{
  position: relative;
  top: 9rpx;
  margin-right: 6rpx;
}
.header .d{
  height: 10rpx;
  width: 10rpx;
  background: #e75f5b;
  display: inline-block;
  margin-left: 3rpx;
  margin-right: 8rpx;
  position: relative;
  border-radius: 50rpx;
  overflow: hidden;
  top: -15rpx;
}

.hr{
  height: 18rpx;
  background: #e8e8e8;
}


.list-mod{
  padding: 0 30rpx;
  background: #fff;
  overflow: hidden;
}

.list-mod .title{
  font-weight: bold;
  font-size: 40rpx;
  color: #000000;
  line-height: 40rpx;
  margin-top: 38rpx;
  margin-bottom: 33rpx;
}
.list-mod .list-item{
  height: 136rpx;
  position: relative;
}

.list-item .icon{
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  margin: auto;
}

.list-item .text-container{
  margin-left: 84rpx;
}

.list-item .right-container{
  position: absolute;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  height: 40rpx;
}

.list-item .button{
  height: 40rpx;
  line-height: 40rpx;
  font-size: 26rpx;
  width: 90rpx;
  background: #fff;
  border: #ffc000 solid 1rpx;
  color: #ffc000;
  text-align: center;
  border-radius: 10rpx;
  padding: 0;
}

.list-item .text-container .t{
  font-size: 30rpx;
  color: #000000;
  padding-top: 35rpx;
  font-weight: bold;
  line-height: 30rpx;
}
.list-item .text-container .desc{
  padding-top: 15rpx;
  font-size: 26rpx;
  color: #999999;
}

.list-item+.list-item{
  border-top: #e5e5e5 solid 1rpx;
}

.precision-container{
  color: #000000;
  font-size: 22rpx;
  width: 90rpx;
  text-align: center;
}

.precision{
  height: 10rpx;
  background: #dddddd;
  border-radius: 30rpx;
  overflow: hidden;
}
.bar{
  height: 10rpx;
  background: #ffc000;
}
.bottom-list{
  margin-bottom: 26rpx;
}

.bottom-list .list-item .t{
  padding: 0;
  line-height: 109rpx;
  font-size: 30rpx;
}
.bottom-list .list-item{
  height: 109rpx;
  border-top: 0;
  border-bottom: #e5e5e5 solid 1rpx;
  position: relative;
}

.bottom-list .list-item .right-container{
  color: #ffc000;
  font-size: 26rpx;
}

.bottom-list .list-item .icon_hot{
  right: 0;
  left: auto;
}
.bainji_emptybox{
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  width: 590rpx;
}
.list-item .buttonsign {
height:40rpx;
line-height:40rpx;
font-size:26rpx;
width:90rpx;
background:#ffc000;
border:#ffc000 solid 1rpx;
color:#fff;
text-align:center;
border-radius:10rpx;
padding:0;
}
.sharebtn{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 9;
  background: pink;
}