/* pages/broadcast/broadcast.wxss */
@import "../center/center.wxss";
.xuhao{
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 35rpx;
  color: #ffc000;
  text-align: center;
  border: 1px solid #ffc000;
  border-radius: 50%;
  position: absolute;
  left: 4%;
  top: 50%;
  margin-top: -35rpx;
}
.bgview{
  position: fixed;
  text-align: center;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: url("data:image/jpg;base64,") no-repeat;
  background-size: 100% 100%;
}
.title{
  font-size: 36rpx;
  color: #fff;
  padding: 55rpx 5%;
  box-sizing: border-box;
  text-align: center;
}
.headerbox{
  display: inline-block;
  width: 580rpx;
  height: 580rpx;
  background: rgba(255, 255, 255, 0.5);
  border-radius: 50%;
  padding: 28rpx;
  box-sizing: border-box;
}
.header_child{
  width: 100%;
  height: 100%;
  border-radius: 50%;
  overflow: hidden;
}
.header{
  display: block;
  width: 530rpx;
  height: 530rpx;
}
.progressbox{
  padding: 66rpx 20% 20rpx;
  box-sizing: border-box;
  position: relative;
}
.fTime{
  font-size: 22rpx;
  color: #fff;
  position: absolute;
  left: 5%;
  top: 50%;
  margin-top: 9rpx;
}
.totalTime{
  font-size: 22rpx;
  color: #fff;
  position: absolute;
  right: 5%;
  top: 50%;
  margin-top: 9rpx;
}
.bar{
  position: relative;
}
.point{
  content: '';
  display: inline-block;
  width: 32rpx;
  height: 32rpx;
  background: green;
  position: relative;
  z-index: 9;
}
slider{
  margin: 0;
}
.contrlbox{
  padding: 18rpx 100rpx;
  box-sizing: border-box;
  display: box;
  display: -webkit-box;
}
.contrl_child{
  -webkit-box-flex: 1;
  box-flex:1;
  width: 118rpx;
  height: 118rpx;
  margin: 0 6%;
}
.leftBtn{
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
  background-position: 0 0;
}
.rightBtn{
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
  background-position: 0 0;
}
.stopBtn{
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
  background-position: 0 0;
}
.playBtn{
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
  background-position: 0 0;
}
.broadcastNav{
  display: box;
  display: -webkit-box;
  padding: 50rpx 0 30rpx;
}
.bNav_child{
  -webkit-box-flex: 1;
  box-flex:1;
  text-align: center;
  font-size: 26rpx;
  color: #d0d0ce !important;
  border-right: 1px solid #969696;
  height:44rpx !important;
  line-height:44rpx !important;
}
.icon1{
  display: inline-block;
  width: 40rpx;
  height: 38rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
  background-position: 0 0;
  vertical-align:-8rpx;
  margin-right: 15rpx;
}
.icon2{
  display: inline-block;
  width: 40rpx;
  height: 38rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
  background-position: 0 0;
  vertical-align:-8rpx;
  margin-right: 15rpx;
}
.icon3{
  display: inline-block;
  width: 40rpx;
  height: 38rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
  background-position: 0 0;
  vertical-align:-8rpx;
  margin-right: 15rpx;
}
.courseContent{
  background: #fff;
  border-radius: 20rpx 20rpx 0 0;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 9;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  max-height:70%;
}
.closeEmpty{
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 2;
  background: rgba(0, 0, 0, 0.3)
}
.classOver{
  z-index: 3;
}
.ccT1{
  font-size: 32rpx;
  color: #292829;
  border-bottom: 1px solid #ececec;
  padding: 40rpx 5%;
  box-sizing: border-box;
}
.courseList{
  padding:25rpx 8% 25rpx 125rpx;
  box-sizing: border-box;
  position: relative;
  border-bottom: 1px solid #ececec;
}

.courseHeader{
  display: block;
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
  position: absolute;
  left: 5%;
  top: 50%;
  margin-top: -42rpx;
}
.ccT2{
  font-size: 28rpx;
  color: #292829;
  padding-bottom: 15rpx;
}
.ccT3{
  font-size: 22rpx;
  color: #999;
  padding-left: 26rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 19rpx 19rpx;
  background-position: 0 50%;
}
.audition{
  display: inline-block;
  height: 40rpx;
  width: 70rpx;
  text-align: center;
  line-height: 40rpx;
  color: #fff;
  border-radius: 6rpx;
  background: #ffc000;
  font-size: 24rpx;
  margin-left: 30rpx;
}
.closeCourse{
  font-size: 32rpx;
  padding: 40rpx 0;
  text-align: center;
  /* border-top: 1px solid #ececec; */
}
