  width: 100%;
  height: 100%;
  position: relative;
}
.scroybox{
overflow-y: scroll;
width: 100%;
height: 100%;
-webkit-overflow-scrolling: touch;
}
.lectureT1{
  font-size: 30rpx;
  color: #999;
  text-align: left;
}
.closeBtn{
  display: inline-block;
  width: 100rpx;
  height: 100rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
  position: absolute;
  right:6%;
  bottom:6%;
  opacity:0.8;
  z-index: 99;
}
.h2w__main {
margin:0;
padding-top:0;
}
.lock{
  display: inline-block;
  width: 25rpx;
  height: 28rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100% 100%; 
  position: absolute;
  right: 5%;
  top: 50%;
  margin-top: -14rpx;
}
