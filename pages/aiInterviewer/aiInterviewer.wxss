/* pages/aiInterviewer/aiInterviewer.wxss */
.banner{
  display: block;
  width: 100%;
  height: 449rpx;
}
.bottomNav{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  border-top: 1px solid #ddd;
  display: -webkit-box;
  display: box;
  padding: 12rpx 0 5rpx;
  background: #fff;
}
.bottomNav_child{
  -webkit-box-flex: 1;
  box-flex:1;
  width: 33%;
  text-align: center;
}
.c1{
  background: url("data:image/png;base64,") no-repeat;
}
.c2{
  background: url("data:image/png;base64,") no-repeat;
}
.c3{
  background: url("data:image/png;base64,") no-repeat;
}
.navselected .c1{
  background: url("data:image/png;base64,") no-repeat;
}
.navselected .c2{
  background: url("data:image/png;base64,") no-repeat;
}
.navselected .c3{
  background: url("data:image/png;base64,") no-repeat;
}
.icon,.navselected .icon{
  display: inline-block;
  width: 42rpx;
  height: 42rpx;
  background-size: 100%;
}
.navtxt{
  font-size: 20rpx;
  color: #686868;
  line-height: 26rpx;
}
.navselected .navtxt{
  color: #f4ba1c;
}
.aiMain{
  padding: 60rpx 5%;
  box-sizing: border-box;
}
.ruleTitle{
  font-size: 32rpx;
  color: #ffc000;
  position: relative;
  padding: 30rpx 0 30rpx 20rpx;
}
.ruleTitle::before{
  content: '';
  display: inline-block;
  width: 5rpx;
  height: 30rpx;
  background: #ffc000;
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -12rpx;
}
.aiT1{
  font-size: 30rpx;
  color: #888;
  padding: 6rpx 0;
}
.goInterviewer{
  display: block;
  background: #ffc000;
  color: #fff;
  text-align: center;
  padding: 28rpx 0;
  border-radius: 10rpx;
  margin-top: 80rpx;
}