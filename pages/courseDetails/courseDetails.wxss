/*下拉动画  */

.kind-list-item-bd {
  height: 0;
  overflow: hidden;
}
.kind-list-item-bd-show {
  height: auto;
}
.navigator-box {
  transform: translateY(-50%);
  transition: .3s;
}
.navigator-box-show {
  transform: translateY(0);
}
.emptybox{
  width: 100%;
  height: 100rpx;
}
