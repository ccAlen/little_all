/* pages/aiFinishedQuestions/aiFinishedQuestions.wxss */
page{
  background: #f2f2f2;
}
.main{
  padding: 20rpx;
}
.checkedbtn{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  padding: 20rpx 5%;
  box-sizing: border-box;
  border-top: 1px solid #ddd;
  background: #fff;
  z-index: 9;
}
.checket{
  background: #ffc000;
  font-size: 32rpx;
  color: #fff;
  text-align: center;
  height: 85rpx;
  line-height: 85rpx;
  border-radius: 10rpx;
}
.list{
  background: #fff;
  border-radius: 15rpx;
  padding: 50rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
}
.list1{
  background: #fff;
  border-radius: 0;
  padding: 20rpx 50rpx 50rpx;
  box-sizing: border-box;
}
.list2{
  background: #fff;
  border-radius: 0;
  padding: 50rpx 0 10rpx;
  box-sizing: border-box;
}
.fqT1{
  font-size: 30rpx;
  color: #666;
  padding-bottom: 40rpx;
}
.radiobox{
  padding-left: 100rpx;
  position: relative;
}
.stop{
  background: url("data:image/png;base64,") no-repeat;
}
.play{
  background: url("data:image/png;base64,") no-repeat;
}
.radioBtn{
  display: inline-block;
  width: 70rpx;
  height: 70rpx;
  background-size: 100%;
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -35rpx;
}
.radioslide{
  padding: 0 100rpx;
  position: relative;
}
.startTime{
  font-size: 26rpx;
  color: #666;
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -18rpx;
}
.endTime{
  font-size: 26rpx;
  color: #666;
  position: absolute;
  right: 0;
  top: 50%;
  margin-top: -18rpx;
}
slider{
  margin: 0;
}
.emptybox100{
  height: 110rpx;
}