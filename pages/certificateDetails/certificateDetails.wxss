/* pages/certificateDetails/certificateDetails.wxss */
/*下拉动画  */
.yellowtxt{
  color: #ffa537;
}
.yellowtxt text{
  border: 1px solid #ffa537;
  color: #ffa537;
}
.kind-list-item-bd {
  height: 0;
  overflow: hidden;
}
.kind-list-item-bd-show {
  height: auto;
}
.navigator-box {
  /* transform: translateY(-50%);
  transition: .3s; */
  display: none;
}
.navigator-box-show {
  /* transform: translateY(0); */
   display: block;
}

.elseList:last-child{
  margin-bottom: 30rpx;
}
.downicon{
  background: url("data:image/jpg;base64,") no-repeat;
  background-size: 38rpx 17rpx;
  background-position: 93% 60%;
}
.upicon{
  background: url("data:image/jpg;base64,") no-repeat;
  background-size: 38rpx 17rpx;
  background-position: 93% 60%;
}
.elseList{
  display: box;
  display: -webkit-box;
  font-size: 30rpx;
  padding: 10rpx 0 50rpx;
  color: #888;
  line-height: 45rpx;
  position: relative;
}
.moreList{
  font-size: 36rpx;
  padding: 30rpx 0;
  text-align: left;
  color: #30b789;
}
.wrap{
  padding:0 5%;
  box-sizing: border-box; 
}
.mlChild{
  display: inline-block;
  padding: 0 78rpx 0 60rpx;
  position: relative;
}
.xl{
  width: 40rpx;
  height: 40rpx;
  border: 1px solid #30b789;
  font-size: 38rpx;
  color: #30b789;
  text-align: center;
  line-height: 40rpx;
  border-radius: 50%;
  position: absolute;
  left: 0;
  top: 4rpx;
}
.slideUp{
  font-size: 28rpx;
  color: #30b789;
  display: inline-block;
  text-decoration: underline;
  position: absolute;
  bottom:-14rpx;
  right:0;
  padding:15rpx 60rpx;
}
.addbottomline{
   border-bottom: 1px solid #c0c0c0;  
}
.h2w__main{
  padding-top: 0 !important;
}
.backTop{
  width: 80rpx; 
  height:80rpx;
  background: url("data:image/png;base64,") #000 no-repeat;
  background-size: 52%;
  opacity: 0.2;
  position: fixed; 
  right: 20rpx ; 
  bottom: 130rpx; 
  border-radius: 50%;
  box-shadow: 1px 1px 3px #999; 
  line-height: 60rpx; 
  text-align: center;
  background-position:50% 50%;
}
.backTop text{font-size: 45rpx; }