/* pages/textualAssistant_step/textualAssistant_step.wxss */
.suggestLink{
  display: inline-block;
  text-decoration: underline;
  color: #ffc000;
}
.stepbox{
  display: flex;
  padding: 40rpx 5%;
  margin-bottom: 35rpx;
}
.stepChild{
  flex: 1;
  text-align: center;
}
.step{
  font-size: 50rpx;
  color: #cacaca;
  display: inline-block;
  border: 2px solid #cacaca;
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border-radius: 50%;
}
.stepTxt{
  text-align: center;
  font-size: 26rpx;
  color: #999;
  padding: 12rpx 0;
}
.selected .step{
  background: #ffc000;
  color: #fff;
  border: 2px solid #ffc000;
}
.selected .stepTxt{
  color: #ffc000;
}
.taT{
  padding: 60rpx 0;
  border-top: 1px solid #e5e5e5;
}
.taT1{
  font-size: 26rpx;
  color: #666;
  line-height: 45rpx;
  text-align: center;
}
.majorbox{
  border: 1px solid #e5e5e5;
  border-radius: 10rpx 10rpx 0 0;
  padding: 40rpx 20rpx 0;
  box-sizing: border-box;
}
.majorbox1{
  border: 1px solid #e5e5e5;
  border-radius: 10rpx;
  padding: 30rpx 20rpx;
  box-sizing: border-box;
}
.majorList{
  display: flex;
}
.inpBox{
  flex: 5;
  border: 1px solid #666;
  border-radius: 10rpx;
  padding-right: 80rpx;
  position: relative;
  height: 80rpx;
}
.closeList{
  flex: 1;
  text-align: center;
  font-size: 30rpx;
  color: #999;
  height: 80rpx;
  line-height: 80rpx;
}
.closeInp{
  display: inline-block;
  width: 80rpx;
  height: 100%;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 38rpx 38rpx;
  background-position: 50% 50%;
  position: absolute;
  right: 0;
  top: 0;
}
.inp{
  display: block;
  width: 100%;
  padding: 0 20rpx;
  color: #000;
  font-size: 36rpx;
  box-sizing: border-box;
  line-height: 80rpx;
  height: 80rpx
}
.wrong{
  display: inline-block;
  width: 178rpx;
  height: 150rpx;
}
.wrongbox{
  padding: 70rpx 0;
  text-align: center;
}
.wrongTxt{
  font-size: 26rpx;
  color: #666;
  text-align: center;
  line-height: 50rpx;
}
.nextStep{
  height: 100rpx;
  line-height: 100rpx;
  color: #fff !important;
  background: #ffc000 !important;
  text-align: center;
  font-size: 36rpx;
  border-radius: 0 0 10rpx 10rpx;
}
.padB50{
  padding-bottom: 50rpx;
}
.unfocus{
  padding: 25rpx 20rpx;
  font-size: 30rpx;
  color: #666;
}
.unfocused{
  padding: 25rpx 20rpx;
  font-size: 30rpx;
  color: #ffc000;
}
.step3List{
  padding: 30rpx 0;
  text-align: center;
  display: block;
}
.step3T1{
  font-size: 26rpx;
  color: #999;
  padding: 14rpx 0;
}
.step3T2{
  font-size: 40rpx;
  color: #000;
}
.emptybox{
  height: 30rpx;
}
.yscroll{
  overflow-y: scroll;
  max-height: 420rpx;
  -webkit-overflow-scrolling: touch;
}