.banner{
  display: block;
  width: 750rpx;
  height: 500rpx;
}
.bannerbox{
  position: relative;
}
.bannerContent{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  display: -webkit-flex;
  display: flex;
  -webkit-align-items: center;
  align-items: center;
  -webkit-justify-content: center;
  justify-content: center;
}
.starbox{
  padding: 0 90rpx;
  position: relative;
  display: inline-block;
}
.starbox::before{
  content: '';
  display: inline-block;
  width: 70rpx;
  height: 1px;
  background: #fff;
  position: absolute;
  left: 0;
  top: 50%;
}
.starbox::after{
  content: '';
  display: inline-block;
  width: 70rpx;
  height: 1px;
  background: #fff;
  position: absolute;
  right: 0;
  top: 50%;
}
