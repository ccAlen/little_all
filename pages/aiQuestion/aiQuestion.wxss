/* pages/aiQuestion/aiQuestion.wxss */
 .headerbox{
   position: relative;
   padding: 150rpx 8% 0 5%;
   box-sizing: border-box;
 }
 .ban{
   display: block;
   width: 100%;
   height: 450rpx;
   left: 0;
   top: 0;
   position: absolute;
 }
 .questionContent{
   width: 100%;
   background: #fff;
   border-radius: 6rpx;
   position: relative;
   padding: 90rpx;
   box-sizing: border-box;
   box-shadow: 0 25rpx 50rpx -10rpx #e6f0ff;
 }
 .monkey{
   position: absolute;
   bottom: -50rpx;
   right: -36rpx;
   display: inline-block;
   width: 111rpx;
   height: 159rpx;
 }
 .order{
   padding:2rpx 35rpx 7rpx 15rpx;
  font-size:32rpx;
   background: -webkit-linear-gradient(-90deg, #f17e3f, #f7295c); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(-90deg, #f17e3f, #f7295c); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(-90deg, #f17e3f, #f7295c); /* Firefox 3.6 - 15 */
    background: linear-gradient(-90deg, #f17e3f, #f7295c); /* 标准的语法（必须放在最后） */
    display: inline-block;
    position: absolute;
    top: 0;
    left: 0;
    color: #fff;
    border-radius: 0 0 50rpx 0;
 }
 .question{
   font-size: 38rpx;
   color: #555;
   line-height: 60rpx;
 }
 .bottombox{
   text-align: center;
   padding: 60rpx 5%;
   box-sizing: border-box;
 }
 .time{
   display: inline-block;
   font-size: 30rpx;
   color: #999;
   padding: 10rpx 0 10rpx 55rpx;
   background: url("data:image/png;base64,") no-repeat;
   background-size: 34rpx 34rpx;
   background-position: 0 50%;
 }
 .mikeT1{
   font-size: 28rpx;
   color: #999;
   padding-top: 20rpx;
 }
 .mike{
   display: inline-block;
   width: 200rpx;
   height: 200rpx;
   background: url("data:image/png;base64,") no-repeat;
   background-size: 100%;
 }
 .time text{
   padding-left: 15rpx;
 }
 .mikebox{
   margin-top: 20%;
   position: relative;
 }
.circle{
  display: inline-block;
  
  position: absolute;
  left: 50%;
  top: 50%;
  
  border: 2px solid #ffc000;
  border-radius: 50%;
  opacity: 0; 
  -webkit-animation: halo 2s 0s infinite ease-out; 
  -moz-animation: halo 2s 0s infinite ease-out; 
  animation: halo 2s 0s infinite ease-out; 
  z-index: -1;
}
.c3{
  width: 200rpx;
  height: 200rpx;
  margin-top:-107rpx;
  margin-left:-104rpx;
}
.c1{
  width: 248rpx;
  height: 248rpx;
  margin-left:-127rpx;
margin-top:-131rpx;

}
.c2{
  width: 224rpx;
  height: 224rpx;
  margin-left:-115rpx;
  margin-top:-119rpx;
}
 @-webkit-keyframes halo { 
    0% { opacity: 0;-webkit-transform: scale(0.8); }
    50% {opacity: 0.5; }
    100%{opacity: 0;-webkit-transform: scale(1.1); } 
}
@keyframes halo {
    0% { opacity: 0; transform: scale(0.8); }
    50% {opacity: 0.5; }
    100%{opacity: 0;transform: scale(1.1); } 
}
.hide{
  display: none;
}
 .mikeT2{
   font-size: 32rpx;
   color: #999;
   padding-top: 60rpx;
   text-decoration: underline;
 }