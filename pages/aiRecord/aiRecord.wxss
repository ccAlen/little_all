/* pages/aiRecord/aiRecord.wxss */
@import "../aiInterviewer/aiInterviewer.wxss";
.banner{
  display: block;
  width: 100%;
  height: 99rpx;
}
.list{
  display: box;
  display: -webkit-box;
  padding: 50rpx 0 30rpx;
  border-bottom: 1px solid #eee;
}
.list_child{
  -webkit-box-flex: 1;
  box-flex:1;
  font-size: 30rpx;
  color: #999;
}
.lc2{
  text-align: right;
}
.listbox{
  padding: 0 5%;
  box-sizing: border-box;
}
.nothing{
  padding: 100rpx;
  font-size: 35rpx;
  color: #999;
  text-align: center;
}
.topbox{
  display:block;
  width:100%;
  height:99rpx;
  background: rgba(0, 0, 0, 0.5);
  padding: 0 99rpx 0 5%;
  box-sizing: border-box;
  position: relative;
}
.tbT{
  font-size: 32rpx;
  color: #fff;
  line-height: 99rpx;
}
.close{
  display: inline-block;
  width: 99rpx;
  height: 99rpx;
  position: absolute;
  right: 0;
  top: 0;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 38rpx 38rpx;
  background-position: 50% 50%;
}